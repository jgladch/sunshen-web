{
  "name": "sunshen-web",
  "version": "0.0.1",
  "private": true,
  "dependencies": {
    "babel-cli": "6.22.2",
    "babel-preset-es2015": "6.22.0",
    "babel-preset-stage-2": "6.22.0",
    "babel-watch": "2.0.6",
    "body-parser": "1.17.2",
    "compression": "1.6.2",
    "cookie-parser": "1.4.3",
    "cookie-session": "2.0.0-beta.1",
    "dotenv": "4.0.0",
    "express": "4.14.0",
    "express-session": "1.15.3",
    "express-sslify": "1.2.0",
    "google-auth-library": "0.10.0",
    "googleapis": "19.0.0",
    "passport": "0.3.2",
    "passport-google-authcode": "0.1.4"
  },
  "scripts": {
    "start": "concurrently \"babel-watch -L --watch ./server/lib ./server/lib/server.js\" \"npm run client\"",
    "server": "babel-node server/lib/server.js",
    "client": "babel-node start-client.js",
    "lint": "eslint .",
    "postinstall": "if [ \"$NODE_ENV\" = \"production\" ] ; then cd client && npm i && npm run build; fi"
  },
  "devDependencies": {
    "concurrently": "3.1.0",
    "eslint": "3.15.0",
    "eslint-config-airbnb": "14.1.0",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-jsx-a11y": "4.0.0",
    "eslint-plugin-react": "6.9.0"
  }
}
